<!--pages/main/index.wxml-->
<!-- <text>pages/main/index.wxml</text> -->
<view class="container">
<swiper
  indicator-dots="true"
  indicator-color="#eee"
  autoplay="true"
  interval="5000"
  circular="true"
  duration="1000"
  duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="355" height="150" />
    </swiper-item>
  </block>
</swiper>
</view>
